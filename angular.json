          }
        }
      }
    }
  },
  "defaultProject": "app",
  "cli": {
    "packageManager": "npm"
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "demo",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "demo"
    }
  }
}
