  "defaultProject": "@ngx-formly-demo",
  "cli": {
    "packageManager": "yarn"
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "demo",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "demo"
    }
  }
